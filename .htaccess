Options -Indexes

RewriteEngine On
RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-l

RewriteRule ^(.+)$ /geocorr/index/default.php?url=$1 [QSA,L]
RewriteRule ^$ /geocorr/index/default.php [R]

